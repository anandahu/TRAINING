// Online C compiler to run C program online
#include <stdio.h>

int main() {
   int a=1;
   int b=1;
   int c=a||--b;
   int d=a--&&--b;
   printf("a=%d,b=%d,c=%d,d=%d",a,b,c,d);
    return 0;
}



